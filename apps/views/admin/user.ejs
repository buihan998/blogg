<!DOCTYPE html>
<html>

<head>
    <title>Admin</title>

    <%- include layout/head %>
</head>

<body>
    <%- include layout/nav %>

        <div class="panel panel-default">
            <div class="panel-heading">
                User
            </div>

            <div class="panel-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover">
                        <thead>
                            <tr>
                                <th>ID</th>
                                <th>Email</th>
                                <th>First name</th>
                                <th>Last name</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% if (data && data.user) {%>
                                <% for( var i =0; i< data.user.length; i++) {%>
                                    <tr>
                                        <td>
                                            <%= data.user[i].id %>
                                        </td>
                                        <td>
                                            <%= data.user[i].email %>
                                        </td>
                                        <td>
                                            <%= data.user[i].ho %>
                                        </td>
                                        <td>
                                            <%= data.user[i].ten %>
                                        </td>
                                    </tr>
                                    <%}%>
                                        <%}%>
                        </tbody>
                    </table>
                </div>

            </div>

        </div>

        <script type="text/javascript" src="/static/js/post.js"></script>
</body>

</html>